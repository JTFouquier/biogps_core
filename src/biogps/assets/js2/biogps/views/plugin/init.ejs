<h2>plugins</h2>
<table cellspacing='0px'>
    <thead>
    <tr>
        <% for(var attr in Biogps.Models.Plugin.attributes){%>
	        <% if(attr == 'id') continue;%>
	        <th><%= attr%> </th>    
        <%}%>
        <th>Options</th>
    </tr>
    </thead>
    <tbody>
        <%= view('views/plugin/list',{plugins: plugins})%>
    </tbody>
</table>
<h2>New plugin</h2>
<form>
    <% for(var attr in Biogps.Models.Plugin.attributes){ %>
        <% if(attr == 'id') continue;%>
        <div>
            <label><%= attr %> </label><br />
            <input type="text" value="" name="<%= attr %>"/>
        </div>
    <%}%>
    <input type='submit' value='Create'/>
</form>